{
  "name": "danio1024/behat-fixtures-loader",
  "description": "Behat extension to simplify loading entities into your ORM",
  "type": "library",
  "keywords": [ "behat extension", "entity", "fixtures"],
  "homepage": "http://github.com/danio1024/BehatFixturesLoader",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Remiszewski",
      "email": "remiszewski.daniel@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "doctrine/orm": "~2.4",
    "symfony/options-resolver": ">=2.3",
    "symfony/dependency-injection": ">=2.3",
    "symfony/yaml": ">=2.3",
    "behat/behat": ">=3.0",
    "behat/symfony2-extension": ">=2.0",
    "fzaninotto/faker": ">=1.4",
    "doctrine/data-fixtures": "~1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-0": {
      "BehatFixturesLoader": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}